{
    "extends": "eslint:recommended",
    "env": {
        "browser": false,
        "commonjs": false,
        "node": true,
        "jquery": false,
        "es6": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": false
        },
        "sourceType": "module"
    },
    "globals": {
        "bootbox": false
    },
    "plugins": [

    ],
    "rules": {
        "semi": 0,
        "no-unused-vars": 0,
        "no-console": 0
    }
}